<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<div class = "header"><div class = "holder">
	<div class = "left">
		<img class = "logo" src = "img/room/logo.png"/>
	</div>
	<div class = "middle">
			<span class = "functions">
				<span class = "whiteboard"><span>白板</span></span>
				<span class = "document"><span>文档</span></span>
				<span class = "media selected"><span>影音</span></span>
				<div class = "clear-float"> </div>
			</span>
			<span class = "downloads">
				<span class = "app" title = "下载手机客户端">APP</span>
				<span class = "pc" title = "下载PC客户端">PC客户端</span>
				<div class = "clear-float"> </div>
			</span>
		<div class = "clear-float"> </div>
	</div>
	<div class = "right">
		<c:choose>
			<c:when test="${!userToken.loginMark}">
				<span class = "btn login">登录</span>
				<span class = "btn register">注册</span>
			</c:when>
			<c:otherwise>
				<span class = "user-info" style="display: block;">
                <img src = "img/room/user.png"/>
                <label>游客：${userToken.nickName}</label>
            </span>
			</c:otherwise>
		</c:choose>
		<span class = "setting" title = "设置"></span>
	</div>
	<div class = "clear-float"> </div>
</div></div>
